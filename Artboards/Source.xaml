<UserControl x:Class="Netzwerkanalyse.Artboards.Source"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Netzwerkanalyse.Artboards"
             mc:Ignorable="d" 
             d:DesignHeight="150" 
             d:DesignWidth="150">
    <Grid>
        <Ellipse Height="50" Canvas.Left="25" Stroke="Black" Canvas.Top="295" Width="50"/>
        <Line X1="65" Y1="100" X2="65" Y2="150" Stroke="Black"/>
        <Line X1="65" Y1="0" X2="65" Y2="50" Stroke="Black"/>
        <Line X1="35" Y1="55" X2="35" Y2="40" Stroke="Black"/>
        <Line X1="27.5" Y1="47.5" X2="43" Y2="47.5" Stroke="Black"/>
        <Line X1="27.5" Y1="100" X2="43" Y2="100" Stroke="Black"/>
        <Polygon Points="110,100  105,90 115,90" Stroke="Blue" Fill="Blue"/>
        <Line X1="110" Y1="50" X2="110" Y2="90" Stroke="Blue"/>
        <Polygon Points="65,120  60,130 70,130" Stroke="Red" Fill="Red"/>
        <Label Content="Iq" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="75,110,0,0" Width="16" Height="20" FontSize="7"/>
        <Label Content="Uq" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="110,0,0,0" Width="22" Height="20" FontSize="7"/>
    </Grid>
</UserControl>
